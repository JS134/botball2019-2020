void turn_angle(double power, double angle){
  #ifdef roomba
  double goal_angle = get_create_total_angle()+angle;
  double nPower = (goal_angle > get_create_total_angle()) ? power : -power;
  while(goal_angle-get_create_total_angle()<2 || goal_angle-get_create_total_angle()>-2) {
    move(nPower,-nPower);
  }
  stop_move();
  #endif
}
